* {
    margin: 10px;
    background-color: rgb(18, 28, 177);
}
header {
    font-size: 35px;
    color: fuchsia;
}
h1, h3 {
    text-align: center;
    color: rgb(187, 216, 23);
}
.pr, .work, .story {
    text-justify: auto;
    color: lightblue;

}
.container {
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  position: relative;
}
.image-container img{
    width: 400px;
    height: 400px;
    box-sizing: border-box;
    
}
.waterfall {
    width: 150%;
    height: 150%;
}
.transition-image {
    position: absolute;
    width: 500px;
    height: 400px;
    top: 20px;
    left: 20px;
    opacity: 0;
    transition: opacity 0.5s ease;
}
.transition-image:hover {
    opacity: 1;
}
.slider-container {
    display: flex;
    position: relative;
    height: 400px;
    width: 500px;
}
.slider {
    display: flex;
    position: static;
    height: 400px;
    width: 500px;
    top: 0px;
    left: 0px;
    background-image: url(../iguana.HASH_REF_105cc74e8858f6e57d0840f52acdbc69.png);
    animation: slide 20s infinite;
    background-color: black;
}
@keyframes slide {
    25% {
        background-image: url(../iguana.HASH_REF_105cc74e8858f6e57d0840f52acdbc69.png);
    }
    50% {
        background-image: url(../cave.HASH_REF_5844ed4a86d234930e454ad2e9128434.png);
    }
    75% {
        background-image: url(../water-cave.HASH_REF_7b26173d4f31b3ca52fca8517f94cad3.png);
    }
    100% {
        background-image: url(../sunset.HASH_REF_70df8d69528948561d2ca25dcfa46429.png);
    }
}
.button {
    display: inline;
   
}
button {
    background-color: cornsilk; 
}

